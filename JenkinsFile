#!/usr/bin/groovy

node {

env.WORKSPACE = pwd()

  stage("Setup") {
    deleteDir()
    git url: "https://github.com/benhosmer/shiro-4.git"
  }

  def params = readFile "${env.WORKSPACE}/test.properties"

  stage("echo") {
    println $params
    sh 'echo "${params}"'
    sh "echo 'testing'"
    sh "env"
  }
}

