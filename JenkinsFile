#!/usr/bin/groovy

import groovy.json.JsonSlurper

node {
  env.WORKSPACE = pwd()

  stage("Setup") {
    deleteDir()
    git url: "https://github.com/benhosmer/shiro-4.git"
  }

  def propsfile = readFile "${env.WORKSPACE}/test.json"
  def params = new JsonSlurper().parseText(propsfile)

  stage("echo") {
    println params
    sh 'echo "${params}"'
    sh "echo 'testing'"
    sh "env"
  }
}

