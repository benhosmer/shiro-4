#!/usr/bin/groovy

import groovy.json.JsonSlurperClassic

node {
  env.WORKSPACE = pwd()

  stage("Setup") {
    deleteDir()
    git url: "https://github.com/benhosmer/shiro-4.git"
  }

  def propsfile = readFile "${env.WORKSPACE}/test.json"
  def params = new JsonSlurperClassic().parseText(propsfile)

  stage("echo") {
    println "${params.guid}"
  }

  stage("archive") {
    archive "./"
  }
}

